{
  "name": "pokebot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "dev:backend": "dotenv -e .env -- pnpm --filter backend start:dev",
    "prod:backend": "dotenv -e .env -- pnpm --filter backend start:prod",
    "prod:backend:build": "pnpm --filter backend build",
    "dev:frontend": "pnpm --filter frontend start",
    "dev": "concurrently -k \"pnpm dev:backend\" \"pnpm dev:frontend\"",
    "seed": "dotenv -e ../../.env -- ts-node ./prisma/seed.ts",
    "prisma:format": "prisma format --schema=./prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "dotenv-cli": "^8.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "pokenode-ts": "^1.20.0",
    "prisma": "^6.12.0"
  }
}
